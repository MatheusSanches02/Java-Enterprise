package br.com.fiap.revisao.main;

import java.util.ArrayList;
import java.util.List;

import br.com.fiap.revisao.model.Carro;
import br.com.fiap.revisao.model.CarroEsportivo;

public class TesteColecoes {

	public static void main(String[] args) {
		List<Carro> carros = new ArrayList<Carro>();
		
		CarroEsportivo ferrari = new CarroEsportivo("Fearrari", 2021, 300.0);
		CarroEsportivo masseratti = new CarroEsportivo("Masseratti", 2022, 400.0);
		
		carros.add(ferrari);
	}
}
